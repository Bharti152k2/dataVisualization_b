{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "api/index.js"
    }
  ],
  "includeFiles": [
    "database/**/*.js",
    "routes/**/*.js",
    "models/**/*.js",
    "controller/**/*.js"
  ]
}
